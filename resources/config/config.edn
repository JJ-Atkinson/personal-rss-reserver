                             :lotus-eaters/password     #n/ref :lotus-eaters/password
                             :downloads-per-day         0}

                            :personal-rss-feed.feed.feed-host/server
                            {:jetty                    {:port 3000}
                             :db/conn                  #ig/ref :personal-rss-feed.feed.db/conn
                             :s3/public-s3-prefix      #n/ref :s3/public-s3-prefix ;; Only semi secret, but preferable to not post on the internet.
                             :feed/public-feed-address #n/ref :feed/public-feed-address
