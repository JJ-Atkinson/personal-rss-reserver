                            {:jetty                    {:port 3001}
                             :db/conn                  #ig/ref :personal-rss-feed.feed.db/conn
                             :auth                     #ig/ref :personal-rss-feed.admin.auth/auth
                             :s3/public-s3-prefix      #n/ref :s3/public-s3-prefix ;; Only semi secret, but preferable to not post on the internet.
                             :feed/secret-path-segment #n/ref :feed/secret-path-segment ;; Easier to implement with overcast than actual http basic auth
                             :queue                    #ig/ref :personal-rss-feed.queue/queue
                             }}

